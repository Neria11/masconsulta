.social
	position fixed
	right 0
	top 40%
	background white
	width 50px
	box-sizing border-box

	.networks 
		margin 0
		padding 0
		display block
		
		.net 
			display block
			width 100%
			margin 0
			
			a
				background rojo
				width 50px
				height 50px
				display table
				color white
				text-align center
				text-decoration none
				
				&:hover
					background rgba(black,0.5)
					

				span
					display table-cell
					color white
					vertical-align middle